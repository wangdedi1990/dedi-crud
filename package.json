{
  "name": "dedi-crud",
  "version": "1.0.0",
  "description": "A SPA that Create/Read/Update/Delete employees data.",
  "main": "index.js",
  "scripts": {
    "client-npm": "cd client && npm install",
    "server-npm": "cd server && npm install",
    "client-build": "cd client && npm run build",
    "postinstall": "npm run client-npm && npm run server-npm && npm run client-build",
    "server": "cd server && npm run start",
    "client": "cd client && npm run start",
    "start-dev": "npm-run-all -p server client",
    "start": "npm run server"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/wangdedi1990/dedi-crud.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/wangdedi1990/dedi-crud/issues"
  },
  "homepage": "https://github.com/wangdedi1990/dedi-crud#readme",
  "dependencies": {
    "npm-run-all": "^4.1.2"
  }
}